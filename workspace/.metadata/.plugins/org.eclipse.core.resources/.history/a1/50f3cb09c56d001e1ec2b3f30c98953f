package org.opentutorials.javatutorials.compare;

public class DivisionDemo {

	public static void main(String[] args) {
		int a = 10;
		int b = 3;
		
		float c = 10.0F;
		float d = 3.0F;
		
		System.out.println(a/b); //3
		System.out.println(c/d); //3.33333
		System.out.println(a/d); //3.33333
	}

}
